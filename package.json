{
  "name": "sweet-tippy-vue",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/sweet-tippy-vue.umd.cjs",
  "module": "./dist/sweet-tippy-vue.js",
  "exports": {
    ".": {
      "import": "./dist/sweet-tippy-vue.js",
      "require": "./dist/sweet-tippy-vue.umd.cjs"
    },
    "./styles.css": "./dist/style.css"
  },
  "types": "./dist/types.d.ts",
  "version": "0.0.1",
  "description": "Short explanation about the plugin",
  "keywords": ["vue plugin", "vue tooltips", "vue3 tooltips"],
  "homepage": "https://github.com/darkshloser/vue-plugin-test",
  "bugs": {
    "url": "https://github.com/darkshloser/vue-plugin-test/issues",
    "email": "dobromir@gsconsult.io"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:types",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "preview": "vite preview --port 4173"
  },
  "dependencies": {
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "defu": "^6.1.0",
    "tippy.js": "^6.3.7",
    "vue": "^3.2.38",
    "vue-tsc": "^1.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "vite": "^3.0.9"
